{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "openlist-work",
  "main": "src/index.ts",
  "compatibility_date": "2025-08-16",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "vars": {
    "ENABLE_D1": true,
    "REMOTE_D1": ""
  },
  "assets": {
    "directory": "./public",
    "binding": "ASSETS"
  }
}
